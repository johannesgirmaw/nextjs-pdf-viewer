{
  "name": "ai-pdf-viewer",
  "version": "0.1.0",
  "description": "World-class AI-powered PDF viewer with smooth rendering, search, and pluggable AI (embeddings + RAG).",
  "license": "MIT",
  "author": "Yohannes (and GPT-5)",
  "repository": {
    "type": "git",
    "url": "git+https://example.com/ai-pdf-viewer.git"
  },
  "bugs": {
    "url": "https://example.com/ai-pdf-viewer/issues"
  },
  "homepage": "https://example.com/ai-pdf-viewer#readme",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./web-component": {
      "import": "./dist/web-component/index.js",
      "require": "./dist/web-component/index.cjs",
      "types": "./dist/web-component/index.d.ts"
    },
    "./ai": {
      "import": "./dist/ai/index.js",
      "require": "./dist/ai/index.cjs",
      "types": "./dist/ai/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch --onSuccess \"echo Build succeeded\""
  },
  "keywords": [
    "pdf",
    "viewer",
    "ai",
    "rag",
    "search",
    "web-component",
    "pdf.js",
    "typescript"
  ],
  "peerDependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.4",
    "@tensorflow/tfjs": "^4.20.0"
  },
  "peerDependenciesMeta": {
    "@tensorflow-models/universal-sentence-encoder": {
      "optional": true
    },
    "@tensorflow/tfjs": {
      "optional": true
    }
  },
  "dependencies": {
    "pdfjs-dist": "^4.6.82"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
}
